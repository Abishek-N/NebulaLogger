<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Post_Processing_not_supported</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  SObjectType__r.DeveloperName != &apos;Log&apos;,
  SObjectType__r.DeveloperName != &apos;LogEntry&apos;,
  OR(
    NOT(ISBLANK(PostProcessorApexClassName__c)),
    NOT(ISBLANK(PostProcessorFlowApiName__c))
  )
)</errorConditionFormula>
    <errorDisplayField>SObjectType__c</errorDisplayField>
    <errorMessage>Post-processing is not supported for this SObject</errorMessage>
</ValidationRule>
