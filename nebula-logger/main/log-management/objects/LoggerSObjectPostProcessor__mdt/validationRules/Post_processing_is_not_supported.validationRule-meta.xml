<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Post_processing_is_not_supported</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    LoggerSObjectHandlerConfiguration__r.SObjectType__r.DeveloperName != &apos;Log&apos;,
    LoggerSObjectHandlerConfiguration__r.SObjectType__r.DeveloperName != &apos;LogEntry&apos;
)</errorConditionFormula>
    <errorDisplayField>LoggerSObjectHandlerConfiguration__c</errorDisplayField>
    <errorMessage>Post-processors are not supported for this SObject Handler</errorMessage>
</ValidationRule>
